{
  "name": "otus-repo-react",
  "version": "1.0.0",
  "description": "react exercises",
  "main": "index.js",
  "scripts": {
    "start": "npx webpack serve --mode development --open --hot",
    "build": "npx webpack --mode production",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx --fix ./",
    "loki:update": "npm run build-storybook && npx loki update --requireReference --reactUri file:./storybook-static",
    "loki:approve": "npx loki approve",
    "test": "npx jest",
    "test:loki": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sLokhin/otus-repo-react.git"
  },
  "author": "Sergey Lokhin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sLokhin/otus-repo-react/issues"
  },
  "homepage": "https://github.com/sLokhin/otus-repo-react#readme",
